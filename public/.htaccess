RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(addAlbum)$ index.php?route=$1

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(track|album|delete|update|showAlbum|showTrack)/([0-9]+)$ index.php?route=$1&id=$2

